        "com.example.accelstorage.AccelStorageAdvanced": {
            "type": "interface", 
            "name": "com.example.accelstorage.AccelStorageAdvanced", 
            "uuid": "14dd174c-efc0-4ebb-9bb5-3e59379e7b26", 
            "slots": {
                "fetch_batch": {
                    "type": "procedure", 
                    "in": "com.example.accelstorage.TimeRange", 
                    "out": "com.example.accelstorage.AccelBatch", 
                    "stream": null, 
                    "docs": [
                        "Fetch all sample series within the given time range as one batch."
                    ]
                }, 
                "fetch_stream": {
                    "type": "procedure", 
                    "in": "com.example.accelstorage.TimeRange", 
                    "out": "com.example.accelstorage.AccelSeries", 
                    "stream": "out", 
                    "docs": [
                        "Fetch all sample series within the given time range.", 
                        "Sample series are returned as streaming, \"progressive results\"."
                    ]
                }, 
                "store_batch": {
                    "type": "procedure", 
                    "in": "com.example.accelstorage.AccelBatch", 
                    "out": "com.example.accelstorage.TimeRange", 
                    "stream": null, 
                    "docs": [
                        "Bulk upload sample batches."
                    ]
                }, 
                "store_stream": {
                    "type": "procedure", 
                    "in": "com.example.accelstorage.AccelSeries", 
                    "out": "com.example.accelstorage.TimeRange", 
                    "stream": "in", 
                    "docs": [
                        "Bulk upload sample batches.", 
                        "Sample batches are provided as streaming, \"progressive call arguments\""
                    ]
                }, 
                "average_batch": {
                    "type": "procedure", 
                    "in": "com.example.accelstorage.AccelBatch", 
                    "out": "com.example.accelstorage.AccelBatch", 
                    "stream": null, 
                    "docs": [
                        "Compute sliding window average of sample using bulk transformation", 
                        "of batches of samples."
                    ]
                }, 
                "average_stream": {
                    "type": "procedure", 
                    "in": "com.example.accelstorage.AccelSeries", 
                    "out": "com.example.accelstorage.AccelSeries", 
                    "stream": "inout", 
                    "docs": [
                        "Compute sliding window average of sample using a streaming transformation", 
                        "receiving arguments and producing results in a full duplex stream."
                    ]
                }
            }, 
            "docs": [
                "Define an interface for accelerometer storage services."
            ]
        }, 
